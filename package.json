{
  "name": "pangaea-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "prestart": "npm run build:css",
    "start": "cross-env BROWSER=none react-scripts start",
    "prebuild": "cross-env NODE_ENV=production npm run build:css",
    "build": "react-scripts build",
    "cy:open": "cypress open --env configFile=development",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "lint": "eslint --config ./.eslintrc --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx ./src",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "npm-run-all --parallel check-types lint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "cypress": "^6.6.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint-config-kentcdodds": "^17.4.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^9.1.0",
    "husky": "^5.1.3",
    "ignore-loader": "^0.1.2",
    "is-ci-cli": "^2.2.0",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.5.4",
    "nyc": "^15.1.0",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^2.0.3",
    "ts-jest": "^26.5.2",
    "typescript": "~3.8.2"
  }
}